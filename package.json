{
  "name": "RSSchool-NodeJS-2020Q2",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aykuli/RSSchool-NodeJS-2020Q2"
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "dev": "nodemon task1/http.js",
    "lint": "eslint ./ --ignore-path .gitignore --fix",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "generate-DB": "node src/data-generators/generating-db.js"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2",
    "yamljs": "0.3.0",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "chai": "4.3.0",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "author": "Anton Yasko"
}
